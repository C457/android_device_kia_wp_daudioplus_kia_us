<mixer>
  <!-- These are the initial mixer settings -->
  <ctl name="Jack Function" value="Speaker" />
  <ctl name="OUTPUT Mono Switch" value="0" />
  <ctl name="LOUT2 Switch" value="0" />
  <ctl name="ROUT2 Switch" value="0" />

  <path name="speaker">
    <ctl name="Jack Function" value="Speaker" />
    <ctl name="OUTPUT Mono Switch" value="1" />
    <ctl name="LOUT2 Switch" value="1" />
    <ctl name="ROUT2 Switch" value="1" />
  </path>

  <path name="headphone">
    <ctl name="Jack Function" value="HeadPhone" />
    <ctl name="OUTPUT Mono Switch" value="0" />
    <ctl name="LOUT2 Switch" value="1" />
    <ctl name="ROUT2 Switch" value="1" />
  </path>

  <path name="main-mic">
  </path>

  <path name="line-in">
  </path>

</mixer>
